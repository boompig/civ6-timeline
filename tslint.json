{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
		"indent": [true, "tabs"],
        "semicolon": [true, "always"],
        "no-var-keyword": true,
		"prefer-const": true,
		"no-console": false,
		"no-empty-interface": false,
		"forin": false
	},
    "rulesDirectory": []
}